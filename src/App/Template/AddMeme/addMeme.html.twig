{% extends "layout.html.twig" %}
{% block title %}Ajouter un Meme{% endblock %}
{% block content %}
    {% block body %}
        <form action="" method="post">
            <div class="form-group">
                <label for="name">Nom du Meme</label>
                <input type="text" class="form-control" id="name" name="name" placeholder="Nom du Meme">
            </div>
            <div class="form-group">
                <label for="image">Url de l'Image</label>
                <input type="text" class="form-control" id="image" name="image">
            </div>
            <button type="submit" name="addNewMeme" class="btn btn-primary">Partager le Meme</button>
        </form>
    {% endblock %}
{% endblock %}

